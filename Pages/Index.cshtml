@page
@model IndexModel
@{
    //ViewData["Title"] = "Home page";
    List<UserInfo>? users = ViewData["users"] as List<UserInfo>;
    string env = ViewData["Environment"].ToString();
}

<div class="text-center">
    <h1 class="display-4">Welcome from @env</h1>
    <h2 class="display-4">Committing Line</h2>
    <table class="table">
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        @foreach (var user in users)
        {
            <tr>
                <td>@user.ID</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>            
            </tr>
        }
    </table>
    @* <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p> *@
</div>
